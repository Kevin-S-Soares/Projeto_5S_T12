@model WebOdontologista.Models.Dentist
@{
    ViewData["Title"] = "Novo Odontologista";
}

<h2>@ViewData["Title"]</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Name" class="control-label"> </label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div class="form-group">
                <br>
                <input type="submit" value="Adicionar Novo Odontologista" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>